{
  "name": "@cesiumgs/engine",
  "version": "2.4.1",
  "description": "Cesium Analytics SDK is a JavaScript library to extend your Cesium-based apps with GPU-accelerated analysis tools.",
  "license": "SEE LICENSE IN sla-cesium-ion-components.pdf",
  "repository": "https://github.com/CesiumGS/cesium-analytics",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@CesiumGS"
  },
  "keywords": [
    "3D",
    "webgl",
    "geospatial",
    "map",
    "globe"
  ],
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "Source/**",
    "Build/Workers/**",
    "README.md",
    "LICENSE.md"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "sideEffects": [
    "./Source/ThirdParty/**/*",
    "./Source/Widget/*.css",
    "./Source/Workers/*",
    "./Specs/**/*"
  ],
  "dependencies": {
    "@tweenjs/tween.js": "^18.6.4",
    "@zip.js/zip.js": "2.4.x",
    "autolinker": "^4.0.0",
    "bitmap-sdf": "^1.0.3",
    "dompurify": "^3.0.2",
    "earcut": "^2.2.4",
    "grapheme-splitter": "^1.0.4",
    "jsep": "^1.3.8",
    "kdbush": "^4.0.1",
    "ktx-parse": "^0.5.0",
    "lerc": "^2.0.0",
    "mersenne-twister": "^1.1.0",
    "meshoptimizer": "^0.19.0",
    "pako": "^2.0.4",
    "protobufjs": "^7.1.0",
    "rbush": "^3.0.1",
    "topojson-client": "^3.1.0",
    "urijs": "^1.19.7"
  },
  "type": "module",
  "scripts": {
    "coverage": "gulp coverage --workspace @cesiumgs/engine",
    "test": "gulp test --production",
    "postversion": "gulp postversion --workspace @cesiumgs/engine",
    "start": "node server.js --production",
    "start-public": "node server.js --public --production",
    "test-all": "gulp test --all --production",
    "test-webgl": "gulp test --include WebGL --production",
    "test-non-webgl": "gulp test --exclude WebGL --production",
    "test-webgl-validation": "gulp test --webglValidation --production",
    "test-webgl-stub": "gulp test --webglStub --production",
    "test-release": "gulp test --release --production"
  },
  "homepage": "https://cesium.com/platform/cesiumjs/ion-sdk/",
  "author": {
    "name": "Cesium GS, Inc.",
    "url": "https://cesium.com"
  },
  "bugs": {
    "url": "https://github.com/CesiumGS/cesium/issues"
  }
}